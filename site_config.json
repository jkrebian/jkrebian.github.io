{
    "site_name": "John Krebian TeeDoc",
    "site_slogon": "Documentation Project with TeeDoc",
    "site_root_url": "/",
    "site_domain": "jkrebian.github.io",
    "site_protocol": "https",
    "config_template_dir": "config",
    "source": "https://github.com/jkrebian/jkrebian.github.io/blob/main",
    "route": {
        "docs": {
            "/get_started/en/": "docs/get_started/en"
        },
        "pages": {
            "/": "pages/index/en"
        },
        "assets": {
            "/static/": "static",
            "/get_started/assets/": "docs/get_started/assets"
        },
        "blog": {
            "/blog/": "blog"
        }
    },
    "plugins": {
        "teedoc-plugin-markdown-parser":{
            "from": "pypi",
            "config": {
            }
        },
        "teedoc-plugin-theme-default":{
            "from": "pypi",
            "config": {
                "dark": true,
                "show_print_page": true,
                "heading_no_counter": false,
                "env":{
                    "main_color": "#4caf7d"
                }
            }
        },
        "teedoc-plugin-search":{
            "from": "pypi",
            "config": {
                "search_hint": "Search"
            }
        },
        "teedoc-plugin-blog": {
            "from": "pypi",
            "config": {
            }
        },
        "teedoc-plugin-assets": {
            "from": "pypi",
            "config": {
                "header_items": [
                    "/static/css/custom.css"
                ],
                "footer_items": [
                    "/static/js/custom.js"
                ],
                "env":{
                    "main_color": "#4caf7d",
                    "second_color": "#0a7d43"
                }
            }
        }
    },
    "rebuild_changes_delay": 1,
    "robots":{
        "User-agent": "*"
    },
    "layout_root_dir": "layout"
}
